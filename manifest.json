{
  "manifest_version": 3,
  "name": "Markdown Image Resizer",
  "version": "0.1.0",
  "description": "Replace the markdown image tag with a resized image tag",
  "permissions": [
    "contextMenus",
    "scripting",
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
